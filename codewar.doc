fun main(){
    println(twoOldestAges(listOf(4,6,7,98,66)))
    
}


fun twoOldestAges(ages: List<Int>): List<Int> {
        var max1 = Int.MIN_VALUE
        var max2 = Int.MIN_VALUE
        for (age in ages) {
            if (age > max1) {
                max2 = max1
                max1 = age
            } else if (age > max2) {
                max2 = age
            }
        }
        return listOf(max2, max1)
       fun main(){
    println(twoOldestAges(arrayOf(4,6,7,98,66))
}

